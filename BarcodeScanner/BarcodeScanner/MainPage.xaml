<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:BarcodeScanner"
             x:Class="BarcodeScanner.MainPage">

    <ContentPage.Content>
        <StackLayout>
            <Button Text="Scan" Clicked="Scan_Clicked"/>
            <Label Text="" x:Name="license" TextColor="Red" HorizontalOptions="Center"></Label>
            <Label Text="" x:Name="full_name" TextColor="Red" HorizontalOptions="Center"></Label>
            <Label Text="" x:Name="last_name" TextColor="Red" HorizontalOptions="Center"></Label>
            <Label Text="" x:Name="first_middle" TextColor="Red" HorizontalOptions="Center"></Label>
            <Label Text="" x:Name="first_name" TextColor="Red" HorizontalOptions="Center"></Label>
            <Label Text="" x:Name="middle_name" TextColor="Red" HorizontalOptions="Center"></Label>
            <Label Text="" x:Name="address" TextColor="Red" HorizontalOptions="Center"></Label>
            <Label Text="" x:Name="city" TextColor="Red" HorizontalOptions="Center"></Label>
            <Label Text="" x:Name="state" TextColor="Red" HorizontalOptions="Center"></Label>
            <Label Text="" x:Name="zip" TextColor="Red" HorizontalOptions="Center"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
